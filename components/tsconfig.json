{
    "compilerOptions": {
        "baseUrl": ".",
        "outDir": "./lib/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "noUnusedLocals": true,
        "declaration": true,
        "sourceMap": true,
        "lib": [
            "es2017",
            "dom"
        ],
        "paths": {
            "magellantoo-components/lib/*": [
                "./src/*"
            ]
        }
    },
    "include": [
        "./src/**/*"
    ]
}